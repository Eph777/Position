# PostgreSQL Database Configuration
# Copy this file to .env and update with your actual credentials

# Database host (use localhost for local development, or IP/hostname for remote server)
DB_HOST=localhost

# Database name
DB_NAME=luanti_db

# Database user
DB_USER=postgres

# Database password (use simple password for development)
DB_PASS=postgres123

# Database port (default PostgreSQL port is 5432)
DB_PORT=5432
